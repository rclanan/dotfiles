[user]
	name = Ray Clanan
	email = 115944+rclanan@users.noreply.github.com
[core]
	editor = vim
	autocrlf = input
	whitespace = fix,-indent-with-non-tab,trailing-space,cr-at-eol
	excludesfile = /home/rclanan/.gitignore_global
[color]
	ui = true
	branch = auto
	diff = auto
	status = auto
[diff]
	algorithm = histogram
	colorMoved = zebra
[merge]
	conflictstyle = diff3
[rebase]
	autosquash = true
	autostash = true
[push]
	default = current
	autoSetupRemote = true
[pull]
	rebase = true
[alias]
	st = status -sb
	co = checkout
	br = branch
	ci = commit
	unstage = reset HEAD --
	last = log -1 HEAD
	visual = !gitk
	lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	ll = log --pretty=format:\"%C(yellow)%h%Cred%d %Creset%s%Cblue [%cn]\" --decorate --numstat
	undo = reset --soft HEAD~1
	amend = commit --amend --no-edit
	wip = commit -am \"WIP\"
	fixup = commit --fixup
	branches = branch -a --sort=-committerdate
[commit]
	template = /home/rclanan/.gitmessage
